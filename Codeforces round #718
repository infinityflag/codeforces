typedef long long ll;
using namespace std;
#define mod 10007
int a[10000001];
int main(){
    int t;
    cin >> t;
    while(t--){
        ll n;
        cin >> n;
        if(n%2050==0){
            cout << (n/2050)<<'\n';
        }
        else{
            cout << -1 << '\n';
        }
        
    }
}


b.

#include <iostream>
#include <algorithm>
#include <iomanip>
#include <queue>
#include <stack>
#include <set>
#include <map>
#include <cstdlib>
#include <cstdio>
#include <vector>
#include <stdlib.h>
#include <cstring>
#include <cmath>
#include <math.h>
#include <string>
typedef long long ll;
using namespace std;
int a[5005][5005];
int main(){
    int t;
    cin >> t;
    while(t--){
        int n,m,x;
        cin >> n >> m;
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                cin >> a[i][j];
            }
        }
        for(int i=0;i<n;i++){
            sort(a[i], a[i]+m);
        }
        for(int k=n; k>=1; k--){
            x = 1;
            for(int i=0;i<n;i++){
                if(a[i][1] < a[x][1]){
                    x = i;
                }
                b[x][k] = a[x][1];
            }
        }
        
    }
}


