typedef long long ll;
using namespace std;
long long lim = 1e8;
int cnt[5*100000];
vector<vector<int<int> > a;
int cnta = 0,cntb=0,cntc=0;
int main(){
    int t;
    cin >> t;
    while(t--){
        int n,m;
        cin >> n >> m;
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                cin >> a[i][j];
            }
        }
        for(int i=0;i<n;i++){
            sort(a[i].begin(), a[i].end());
            
        }
    }
}

